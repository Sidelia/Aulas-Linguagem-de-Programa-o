#include<stdio.h>

// Aula 4 ponteiros "O que ser√° impresso pelo programa abaixo?" 

	int main (){
		int a [] = {1,2,3,4,5};
		int *b, i;
		
		b=a;
		printf("Conteudo de b:");
		for(i=0;i<5;i++){
			printf("%d, ", b[i]);
			b[i]=i*i;
		}
		printf("\nConteudo de a: ");
		for(i=0; i<5; i++){
			printf("%d, ", a [i]);
		}
	}
	
